{
  "name": "gauge-ts",
  "version": "0.1.1",
  "description": "Typescript language runner for gauge",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build": "npm run lint && tsc",
    "test:coverage": "jest --verbose --coverage --detectOpenHandles --forceExit",
    "codecov": "codecov",
    "test": "jest --verbose --detectOpenHandles --forceExit",
    "gen-proto": "sh genproto.sh",
    "prepublishOnly": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bugdiver/gauge-ts.git"
  },
  "keywords": [
    "gauge",
    "Acceptance Test",
    "Typescript",
    "Automation"
  ],
  "author": "BugDiver <vinayshankar00@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bugdiver/gauge-ts/issues"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.9",
    "google-protobuf": "^3.21.2",
    "klaw-sync": "^6.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.9",
    "@types/jest": "^29.5.7",
    "@types/klaw-sync": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "@types/uuid": "^9.0.6",
    "codecov": "^3.8.2",
    "grpc-tools": "^1.12.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "jest": "^29.7.0",
    "jest-mock-process": "^2.0.0",
    "jest-ts-auto-mock": "^2.1.0",
    "ts-auto-mock": "^3.7.1",
    "ts-jest": "^29.1.1",
    "ttypescript": "^1.5.15"
  }
}
